<h1>Edit <span>Profile</span></h1>

<form action="/profile/img-upload" method="post" enctype="multipart/form-data"> 
    
    <label for="fullName"></label>
    <input type="text" name="fullName" id="fullName" placeholder="Name">

    <label for="profileInfo"></label>
    <textarea id="profileInfo" name="profileInfo" rows="4" cols="23.4" placeholder="About you!"></textarea>
    
    
    <label for="profileImg">Image:<label>
    <input type="file" name="profileImg"/>


    <button class='sign-up-button' type="submit">Edit User</button>
</form>
